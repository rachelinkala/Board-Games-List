<html>
  <head>
    <% include ./partials/head %>
  </head>
  <body>
    <header>
      <% include ./partials/header %>
    </header>
    <div class="container">
      <% var url = "/board_games/" + board.id + "?_method=PUT" %>
      <form method="POST" action=<%= url %> >
        <input required name="title" value=<%= board.title %> />
        <a href="/board_games" class="btn red">Cancel</a>
        <button class="btn">Save</button>
      </form>
    </div>
  </body>
</html>